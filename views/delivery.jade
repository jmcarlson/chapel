extends layout

block content
	.container
		//- Header menu
		.page-header.h3 Email Queue
		.delivery-container
			h4#del-lead-id.delivery Lead Id
			h4#del-date.delivery Delivery Date
			h4#del-status.delivery Delivery Status
		for delivery in results
			.delivery-container
				.delivery #{delivery.lead_id}
				.delivery #{delivery.delivery}
				.delivery #{delivery.status}
